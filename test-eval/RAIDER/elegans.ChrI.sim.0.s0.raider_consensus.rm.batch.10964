#!/bin/bash -l
#PBS -N repmask.1
#PBS -l nodes=1:ppn=4
#PBS -l walltime=25:00:00
#PBS -o EVAL/RAIDER/elegans.ChrI.sim.0.fa.repmask.stdout
#PBS -e EVAL/RAIDER/elegans.ChrI.sim.0.fa.repmask.stderr
cd $PBS_O_WORKDIR
module load RepeatMasker; module load python-3.3.3; RepeatMasker -nolow -lib EVAL/RAIDER/elegans.ChrI.sim.0.s0.raider_consensus.fa -pa 4 -dir EVAL/RAIDER EVAL/SOURCE_DATA/elegans.ChrI.sim.0.fa